{
    "theme": {
        "name": "Steampad",
        "parentTheme": "Darkpad",
        "vars": {},
        "colors": {
            "white": "FFFFFF",
            "dark": "1F2329",
            "light": "FEFEFE",
            "greenDim": "7EA64B",
            "green": "31A633",
            "greenBright": "3DC03A",
            "greenmed": "89AE05",
            "greenmeddim": "7A9C04",
            "blue": "3D668F",
            "brightblue": "46B4F5",
            "darkblue": "1E4266",
            "lightblue": "59AECE",
            "brightmidblue": "549DC7",
            "disabledlightblue": "355D79",
            "dimblue": "283B47",
            "disabledblue": "1D2E3F",
            "gray": "646E70",
            "lightgray": "C8D0BF",
            "default": "{{dark}}",
            "background": "{{dark}}",
            "text": "{{light}}",
            "textLight": "{{lightgray}}",
            "accent": "{{blue}}",
            "accentLight": "{{lightblue}}",
            "accentDark": "{{darkblue}}",
            "border": "{{blue}}",
            "borderHover": "{{lightblue}}",
            "linkText": "{{greenDim}}",
            "inputText": "{{dark}}"
        },
        "themeAssets": {
            "logo": "",
            "spinner": "Graphics\\Spinner-Steam.gif"
        },
        "buttons": {
            "styles": {
                "normal": {
                    "enabled": {
                        "backgroundColor": "{{dimblue}}",
                        "textColor": "{{brightblue}}",
                        "borderColor": "{{dimblue}}",
                        "borderWidth": 0,
                        "shape": "ButtonShape"
                    },
                    "disabled": {
                        "backgroundColor": "{{disabledblue}}",
                        "textColor": "{{disabledlightblue}}",
                        "borderColor": "{{disabledblue}}",
                        "borderWidth": 0,
                        "shape": "ButtonShape"
                    },
                    "hovered": {
                        "backgroundColor": "{{brightmidblue}}",
                        "textColor": "{{light}}",
                        "borderColor": "{{brightmidblue}}",
                        "borderWidth": 0,
                        "shape": "ButtonShape"
                    }
                },
                "primary": {
                    "enabled": {
                        "backgroundColor": "{{greenmeddim}}",
                        "textColor": "{{light}}",
                        "borderColor": "{{greenmeddim}}",
                        "borderWidth": 0,
                        "shape": "ButtonShape"
                    },
                    "disabled": {
                        "backgroundColor": "{{brightmidblue}}",
                        "textColor": "{{light}}",
                        "borderColor": "{{brightmidblue}}",
                        "borderWidth": 0,
                        "shape": "ButtonShape"
                    },
                    "hovered": {
                        "backgroundColor": "{{greenmed}}",
                        "textColor": "{{white}}",
                        "borderColor": "{{greenmed}}",
                        "borderWidth": 0,
                        "shape": "ButtonShape"
                    }
                },
                "titlebar": {
                    "enabled": {
                        "backgroundColor": "{{background}}",
                        "textColor": "{{textLight}}",
                        "borderColor": "{{border}}",
                        "borderWidth": 0,
                        "shape": "SymbolButtonShape"
                    },
                    "disabled": {
                        "backgroundColor": "{{background}}",
                        "textColor": "{{textLight}}",
                        "borderColor": "{{background}}",
                        "borderWidth": 0,
                        "shape": "SymbolButtonShape"
                    },
                    "hovered": {
                        "backgroundColor": "{{background}}",
                        "textColor": "{{text}}",
                        "borderColor": "{{background}}",
                        "borderWidth": 0,
                        "shape": "SymbolButtonShape"
                    }
                }
            }
        }
    }
}
